<h1>Sign Up</h1>
<p>Find me in app/views/users/new.html.erb</p>

<%= link_to 'Back', interns_path %>


<%= form_for @intern do |f| %>
  <% if @intern.errors.any? %>
    <div class="error_messages">
      <h2>Form is invalid</h2>
      <ul>
        <% for message in @intern.errors.full_messages %>
          <li><%= message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>
  <div class="field">
    <%= f.label :first_name %><br/>
    <%= f.text_field :first_name %>
  </div>
  <div class="field">
    <%= f.label :last_name %><br/>
    <%= f.text_field :last_name %>
  </div>
  <div class="field">
    <%= f.label :dob %><br/>
    <%= f.text_field :dob %>
  </div>

  <div class="actions"><%= f.submit %></div>
<% end %>