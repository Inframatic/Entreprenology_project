<%= form_for @startup do |f| %>
  <% if @startup.errors.any? %>
    <div class="error_message">
      <h2>Form is invalid</h2>
      <ul>
      <% for message in @user.errors.full_messages %>
          <li><%= message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :password %><br/>
    <%= f.password_field :password %>
  </div>
  <div class="field">
    <%= f.label :password_confirmation %><br/>
    <%= f.password_field :password_confirmation %>
  </div>
  <div class="field">
    <%= f.label :company_name %><br>
    <%= f.text_field :company_name %>
  </div>
  <div class="field">
    <%= f.label :founded %><br>
    <%= f.text_field :founded %>
  </div>
  <div class="field">
    <%= f.label :contact_person %><br>
    <%= f.text_field :contact_person %>
  </div>
  <div class="field">
    <%= f.label :contact_person_email %><br>
    <%= f.text_field :contact_person_email %>
  </div>
   <div class="field">
    <%= f.label :industry %><br>
    <%= f.text_field :industry %>
  </div>
  <div class="field">
    <%= f.label :city %><br>
    <%= f.text_field :city %> 
  </div>
  <div class="field">
    <%= f.label :country %><br>
    <%= f.text_field :country %> 
  </div>
  <div class="field">
    <%= f.label :postal_code %><br>
    <%= f.text_field :postal_code %> 
  </div>
  <div class="field">
    <%= f.label :number_of_employees %><br>
    <%= f.text_field :number_of_employees %> 
  </div>
  <div class="field">
    <%= f.label :logo %><br>
    <%= f.text_field :logo %> 
  </div>
  <div class="field">
    <%= f.label :banner %><br>
    <%= f.text_field :banner %>
  </div>
  <div class="field">
    <%= f.label :description %><br>
    <%= f.text_field :description %> 
  </div>
  <div class="action">
    <%= f.submit "Save" %>
  </div>
<% end %>